   "source": [
    "def max_steal(val):\n",
    "    prev1 = 0\n",
    "    prev2 = 0\n",
    "    for num in val:\n",
    "        temp = prev1\n",
    "        prev1 = max(prev2 + num, prev1)\n",
    "        prev2 = temp\n",
    "    return prev1\n",
    "val = [8, 6, 1, 7, 8, 1, 5]\n",
    "print(\"Maximum stolen value:\", max_steal(val))"
   ]
